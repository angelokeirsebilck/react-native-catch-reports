{
  "build": {
    "production": {
      "releaseChannel": "production",
      "env": {
        "APP_ENV": "production"
      }
    },
    "preview": {
      "releaseChannel": "preview",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_ENV": "preview"
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    }
  },
  "cli": {
    "version": ">= 0.47.0"
  }
}
